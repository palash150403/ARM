{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "networkSecurityGroups_dev_propel_appservice_nsg_name": {
      "type": "string",
      "defaultValue": "mynsg"
    }
  },
  "resources": [
    {
      "apiVersion": "2024-05-01",
      "location": "eastus",
      "name": "[parameters('networkSecurityGroups_dev_propel_appservice_nsg_name')]",
      "properties": {
        "securityRules": [
          {
            "id": "[resourceId('Microsoft.Network/networkSecurityGroups/securityRules', parameters('networkSecurityGroups_dev_propel_appservice_nsg_name'), 'app-api-access')]",
            "name": "app-api-access",
            "properties": {
              "access": "Allow",
              "destinationAddressPrefix": "10.1.1.0/24",
              "destinationAddressPrefixes": [],
              "destinationPortRange": "*",
              "destinationPortRanges": [],
              "direction": "Inbound",
              "priority": 100,
              "protocol": "Tcp",
              "sourceAddressPrefix": "10.1.1.0/24",
              "sourceAddressPrefixes": [],
              "sourcePortRange": "*",
              "sourcePortRanges": []
            },
            "type": "Microsoft.Network/networkSecurityGroups/securityRules"
          }
        ]
      },
      "type": "Microsoft.Network/networkSecurityGroups"
    }
  ]
}
