{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {},
  "resources": [
    {
      "name": "deploymentA",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "https://<storage-uri>/templateA.json",
          "contentVersion": "1.0.0.0"
        }
      }
    },
    {
      "name": "deploymentB",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "dependsOn": ["deploymentA"],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "https://<storage-uri>/templateB.json",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "vnetId": {
            "value": "[reference('deploymentA').outputs.vnetId.value]"
          }
        }
      }
    }
  ]
}
