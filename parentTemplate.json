{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {},
  "resources": [
    {
      "name": "deploymentA",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "https://raw.githubusercontent.com/palash150403/ARM/refs/heads/main/templateA.json",
          "contentVersion": "1.0.0.0"
        }
      }
    },
    {
      "name": "deploymentB",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "dependsOn": ["deploymentA"],
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "https://raw.githubusercontent.com/palash150403/ARM/refs/heads/main/templateB.json",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "virtualNetworks_dev_propel_vnet_name": {
            "value": "[reference('deploymentA').outputs.virtualNetworks_dev_propel_vnet_name.value]"
          }
        }
      }
    }
  ],
  "outputs": {
    "messageFromLinkedTemplate": {
      "type": "string",
      "value": "[reference('deploymentA').outputs.virtualNetworks_dev_propel_vnet_name.value]"
    }
  }
